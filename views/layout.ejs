<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="utf-8" />
    <title><%- title %> | Cms</title>
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <meta content="Demo Tours cms in Node" name="description" />
    <link rel="shortcut icon" href="assets/images/favicon.ico" />
    <%- HeaderCss %> <%- include ('./Partials/HeaderStyle') %>
    <script src="/assets/js/functions.js"></script>
  </head>

  <body data-layout="vertical" data-topbar="dark">
    <div id="layout-wrapper">
      <%- include ('./Partials/Topbar') %> <%- include ('./Partials/Sidebar') %>

      <div class="main-content">
        <div class="page-content">
          <div class="container-fluid"><%- breadcrumb %> <%- body %></div>
        </div>
        <%- include ('./Partials/Footer') %>
      </div>
    </div>

    <%- include ('./Partials/Rightbar') %> <%- include ('./Partials/VendorScript') %> <%- FooterJs %>

    <script>
      sessionStorage.setItem('is_visited', 'layout-direction-ltr');
    </script>

    <script src="/assets/js/app.js"></script>

    <%- BottomJs %>
  </body>
</html>
